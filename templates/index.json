        "custom_liquid": "<div class=\"cat-section\">\n  <h2 class=\"cat-section__title\">SHOP BY CATEGORY</h2>\n  <div class=\"cat-grid\">\n    {%- for collection in collections -%}\n      {%- if collection.all_products_count > 0 and collection.handle != 'all' and collection.handle != 'frontpage' -%}\n        {%- unless collection.handle contains 'ford' or collection.handle contains 'chevy' or collection.handle contains 'dodge' or collection.handle contains 'gmc' or collection.handle contains 'toyota' or collection.handle contains 'ram' or collection.handle contains 'jeep' or collection.handle contains 'nissan' -%}\n        <a href=\"{{ collection.url }}\" class=\"cat-tile\">\n          {% if collection.handle contains 'bull' or collection.handle contains 'grille' %}\n            <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"#F39C12\" stroke-width=\"1.5\" width=\"28\" height=\"28\"><path d=\"M12 22s8-4 8-10V5l-8-3-8 3v7c0 6 8 10 8 10z\"/><path d=\"M8 11h8M8 8h8M8 14h8\"/></svg>\n          {% elsif collection.handle contains 'chase' or collection.handle contains 'rack' or collection.handle contains 'sport' %}\n            <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"#F39C12\" stroke-width=\"1.5\" width=\"28\" height=\"28\"><rect x=\"2\" y=\"7\" width=\"20\" height=\"10\" rx=\"1\"/><circle cx=\"7\" cy=\"12\" r=\"2\"/><circle cx=\"17\" cy=\"12\" r=\"2\"/><path d=\"M2 10h20M7 7V5M17 7V5\"/></svg>\n          {% elsif collection.handle contains 'side' or collection.handle contains 'nerf' or collection.handle contains 'step' %}\n            <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"#F39C12\" stroke-width=\"1.5\" width=\"28\" height=\"28\"><path d=\"M4 18h16M4 18v-4h16v4M6 14v-2h4v2M14 14v-2h4v2\"/><circle cx=\"8\" cy=\"20\" r=\"1.5\"/><circle cx=\"16\" cy=\"20\" r=\"1.5\"/></svg>\n          {% elsif collection.handle contains 'tonneau' or collection.handle contains 'cover' %}\n            <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"#F39C12\" stroke-width=\"1.5\" width=\"28\" height=\"28\"><rect x=\"2\" y=\"8\" width=\"20\" height=\"10\" rx=\"1\"/><path d=\"M2 8l4-4h12l4 4M8 4v4M16 4v4M12 4v4\"/></svg>\n          {% else %}\n            <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"#F39C12\" stroke-width=\"1.5\" width=\"28\" height=\"28\"><path d=\"M20 7l-8-4-8 4m16 0l-8 4m8-4v10l-8 4M4 7l8 4M4 7v10l8 4m0-10v10\"/></svg>\n          {% endif %}\n          <span>{{ collection.title }}</span>\n        </a>\n        {%- endunless -%}\n      {%- endif -%}\n    {%- endfor -%}\n  </div>\n</div>",
