      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<div class=\"cat-section\">\n  <h2 class=\"cat-section__title\">SHOP BY CATEGORY</h2>\n  <div class=\"cat-grid\">\n    {%- for collection in collections -%}\n      {%- if collection.all_products_count > 0 and collection.handle != 'all' and collection.handle != 'frontpage' -%}\n        {%- unless collection.handle contains 'ford' or collection.handle contains 'chevy' or collection.handle contains 'dodge' or collection.handle contains 'gmc' or collection.handle contains 'toyota' or collection.handle contains 'ram' or collection.handle contains 'jeep' or collection.handle contains 'nissan' -%}\n        <a href=\"/collections/all?category={{ collection.handle }}\" class=\"cat-tile\">\n          {% if collection.handle contains 'bull' or collection.handle contains 'grille' %}\n            <svg viewBox=\"0 0 32 32\" fill=\"none\" stroke=\"#F39C12\" stroke-width=\"1.4\" width=\"28\" height=\"28\">\n      <path d=\"M6 28V12c0-2 1-4 3-5l3-2V3h8v2l3 2c2 1 3 3 3 5v16\"/>\n      <path d=\"M6 16h20M6 21h20\"/>\n      <path d=\"M11 12v-1M16 12v-1M21 12v-1\"/>\n      <circle cx=\"16\" cy=\"9\" r=\"1.5\"/>\n    </svg>\n          {% elsif collection.handle contains 'chase' or collection.handle contains 'rack' or collection.handle contains 'sport' %}\n            <svg viewBox=\"0 0 32 32\" fill=\"none\" stroke=\"#F39C12\" stroke-width=\"1.4\" width=\"28\" height=\"28\">\n      <path d=\"M4 28V10h4V4h16v6h4v18\"/>\n      <path d=\"M8 10h16\"/>\n      <path d=\"M10 4v6M22 4v6\"/>\n      <path d=\"M12 16h8M12 21h8\"/>\n      <circle cx=\"16\" cy=\"7\" r=\"1.5\"/>\n      <path d=\"M4 15h4M24 15h4\"/>\n    </svg>\n          {% elsif collection.handle contains 'side' or collection.handle contains 'nerf' or collection.handle contains 'step' %}\n            <svg viewBox=\"0 0 32 32\" fill=\"none\" stroke=\"#F39C12\" stroke-width=\"1.4\" width=\"28\" height=\"28\">\n      <rect x=\"2\" y=\"14\" width=\"28\" height=\"5\" rx=\"2.5\"/>\n      <path d=\"M6 14v-3h6v3M20 14v-3h6v3\"/>\n      <circle cx=\"9\" cy=\"22\" r=\"2.5\"/><circle cx=\"23\" cy=\"22\" r=\"2.5\"/>\n      <path d=\"M2 19v3h4.5M15.5 22h5M25.5 22H30v-3\"/>\n    </svg>\n          {% elsif collection.handle contains 'tonneau' or collection.handle contains 'cover' %}\n            <svg viewBox=\"0 0 32 32\" fill=\"none\" stroke=\"#F39C12\" stroke-width=\"1.4\" width=\"28\" height=\"28\">\n      <rect x=\"3\" y=\"12\" width=\"26\" height=\"14\" rx=\"1\"/>\n      <path d=\"M3 12l3-6h20l3 6\"/>\n      <path d=\"M11 6v6M16 6v6M21 6v6\"/>\n      <path d=\"M3 19h26M3 23h26\"/>\n    </svg>\n          {% else %}\n            <svg viewBox=\"0 0 32 32\" fill=\"none\" stroke=\"#F39C12\" stroke-width=\"1.4\" width=\"28\" height=\"28\">\n      <path d=\"M4 22h24l-2-8H6L4 22z\"/>\n      <path d=\"M8 14l2-6h12l2 6\"/>\n      <circle cx=\"9\" cy=\"25\" r=\"2.5\"/><circle cx=\"23\" cy=\"25\" r=\"2.5\"/>\n      <path d=\"M11.5 25h9\"/>\n    </svg>\n          {% endif %}\n          <span>{{ collection.title }}</span>\n        </a>\n        {%- endunless -%}\n      {%- endif -%}\n    {%- endfor -%}\n  </div>\n</div>",
